<!-- course end -->

<!--Btech Modal -->
<div class="modal fade" id="Btech" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="exampleModalLabel">Btech</h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <form action="/createCourse" method="post">
          <div class="mb-3">
            <label>Name</label>
            <input type="text" name="name" value="<%= n %>" class="form-control">
          </div>
          <div class="mb-3">
            <label>Email</label>
            <input type="email" name="email" value="<%= e %>" class="form-control">
          </div>
          <div class="mb-3">
            <label>Phone</label>
            <input type="number" name="phone" class="form-control">
          </div>
          <div class="mb-3">
            <label>10th Marks</label>
            <input type="number" name="tenth" class="form-control">
          </div>
          <div class="mb-3">
            <label>12th Marks</label>
            <input type="number" name="twelth" class="form-control">
          </div>
          <div class="mb-3">
            <label>Course</label>
            <input type="text" name="course" value="Btech" readonly required class="form-control">
          </div>
          <button type="submit">submit</button>
          
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>

